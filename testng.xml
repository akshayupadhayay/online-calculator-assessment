<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
<listeners>
    <listener class-name="reporting.Listener"/>
</listeners>
    <parameter name="browser" value="chrome"/>
    <test verbose="2" preserve-order="true" name="Online-Calculator">
        <classes>
            <class name="com.xendit.TC.FullScreenCalculatorTest">
                <methods>
                    <include name="TC1_calculatorLaunch"/>
                    <include name="TC2_calculatorVisibility"/>
                    <include name="TC3_getTitle"/>
                    <include name="TC4_subtraction"/>
                    <include name="TC5_addition"/>
                    <include name="TC6_division"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>